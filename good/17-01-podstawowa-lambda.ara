// Lambda - podstawowa lambda bez wykorzystywania zmiennych innych ni≈º argument

int main() {
    [int](int, string) my_func = [int](int x, string s) {
        print(s);
        return x;
    };

    int x = my_func(5, "ZWROC LICZBE 5");
    print(x);
    int y = my_func(6, "Zwroc prosze liczbe 6");
    print(y);

    return 0;
}