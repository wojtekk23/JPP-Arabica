// Break - dwie pętle wewnątrz siebie
// break kończy tylko najbliższą pętlę

int main() {
    int x = 0, y = 0;

    while (true) {
        y = 0;
        while(true) {
            y = y+1;
            print("y:");
            print(y);

            if (y == 5)
                break;
        }
        x = x+1;
        print("x:");
        print(x);

        if (x == 5)
            break;
    }

    return 0;
}